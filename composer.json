{
    "name": "yawik/sitemap",
    "description": "YAWIK module to generate sitemap xml",
    "keywords": [
        "yawik",
        "sitemap",
        "seo"
    ],
    "require": {
        "yawik/core": "^0.34",
        "yawik/jobs": "^0.34",
        "samdark/sitemap": "^2.2",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "cross-solution/phpunit-utils": "^2.0",
        "yawik/composer-plugin": "^1.0",
        "yawik/auth": "^0.34",
        "yawik/settings": "^0.34",
        "yawik/applications": "^0.34",
        "yawik/cv": "^0.34",
        "yawik/organizations": "^0.34",
        "yawik/geo": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mathias Gelhausen",
            "email": "gelhausen@cross-solution.de"
        }
    ],
    "extra": {
        "zf": {
            "module": "Sitemap"
        }
    },
    "autoload": {
        "psr-4": {
            "Sitemap\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": [
            "npm install"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "serve": "php -S localhost:8000 -t test/sandbox/public",
        "start-selenium": "./vendor/bin/selenium-server-standalone -p 4444 -Dwebdriver.chrome.driver=./vendor/bin/chromedriver",
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "platform": {
            "ext-mongo": "1.6.16"
        },
        "process-timeout": 0
    }
}
